{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "semi": "error",
    "no-trailing-spaces": "error",
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-console": "error",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}